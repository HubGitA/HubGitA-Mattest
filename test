 CLAIM_DISBRSMT_DT    DATE,
 CLAIM_DEFICIENCY_CD  CHARACTER( 2 ),
 CLAIM_DEFICIENCY_CD_DT DATE,
 BLNG_ST_CD           CHARACTER( 2 ) NOT NULL WITH DEFAULT,
 DEFICIENCY_SENT_TO_CLNT_DT DATE,
 DEFICIENCY_RECVD_FROM_CLNT_DT DATE,
 DEFICIENCY_SENT_TO_CAV_DT DATE,
 TRN_SUM_CREATED_ON_TMSTP TIMESTAMP( 6 ) WITHOUT TIME ZONE NOT NULL WITH
 DEFAULT,
 TRN_SUM_CREATED_BY_ID CHARACTER( 8 ) NOT NULL WITH DEFAULT,
 TRN_SUM_LAST_UPDT_TMSTP TIMESTAMP( 6 ) WITHOUT TIME ZONE,
 TRN_SUM_LAST_UPDT_BY_ID CHARACTER( 8 ),
 TRN_SUMHIS_CREATED_BY_ID CHARACTER( 8 ) NOT NULL WITH DEFAULT
);

