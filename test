CREATE TABLE asfasf
(
 CLNT_ID              CHARACTER( 3 ) NOT NULL,
 PROCESSING_DATE      DATE NOT NULL WITH DEFAULT,
 a                    CHAR(18) NOT NULL
);

LABEL ON COLUMN asfasf.CLNT_ID IS 'CLNT_ID:';

LABEL ON COLUMN asfasf.PROCESSING_DATE IS 'PROCESSING_DATE:';

LABEL ON COLUMN asfasf.a IS 'a:';

CREATE UNIQUE INDEX XPKasfasf ON asfasf
(
 a                    ASC
);

ALTER TABLE asfasf
 ADD CONSTRAINT XPKasfasf  PRIMARY KEY (a);

CREATE TABLE CAL_ACCT_TYP_LKUP
(
 ACCT_TYPE_CD         CHARACTER( 2 ) NOT NULL,
 ACCT_TYPE_CD_DESC    VARCHAR( 100 ) NOT NULL WITH DEFAULT
);

LABEL ON COLUMN CAL_ACCT_TYP_LKUP.ACCT_TYPE_CD IS 'ACCT_TYPE_CD:';

LABEL ON COLUMN CAL_ACCT_TYP_LKUP.ACCT_TYPE_CD_DESC IS
 'ACCT_TYPE_CD_DESC:';

CREATE UNIQUE INDEX CALATL1 ON CAL_ACCT_TYP_LKUP
(
 ACCT_TYPE_CD         ASC
);

ALTER TABLE CAL_ACCT_TYP_LKUP
 ADD CONSTRAINT ACCT_TYP_CD_LKUP  PRIMARY KEY (ACCT_TYPE_CD);

CREATE TABLE CAL_CLNT_SLA
(
 CLNT_ID              CHARACTER( 3 ) NOT NULL,
 CLNT_PROCESSNG_TYPE_CD CHARACTER( 2 ) NOT NULL,
 PROCESSING_DAYS      INTEGER NOT NULL WITH DEFAULT,
 CREATED_ON_TMSTP     TIMESTAMP( 6 ) WITHOUT TIME ZONE NOT NULL WITH
 DEFAULT,
 CREATED_BY_ID        CHARACTER( 8 ) NOT NULL WITH DEFAULT,
 LAST_UPDT_TMSTP      TIMESTAMP( 6 ) WITHOUT TIME ZONE,
 LAST_UPDT_BY_ID      CHARACTER( 8 )
);

LABEL ON COLUMN CAL_CLNT_SLA.CLNT_ID IS 'CLNT_ID:';

LABEL ON COLUMN CAL_CLNT_SLA.CLNT_PROCESSNG_TYPE_CD IS
 'CLNT_PROCESSNG_TYPE_CD:';

LABEL ON COLUMN CAL_CLNT_SLA.PROCESSING_DAYS IS 'PROCESSING_DAYS:';

LABEL ON COLUMN CAL_CLNT_SLA.CREATED_ON_TMSTP IS 'CREATED_ON_TMSTP:';

LABEL ON COLUMN CAL_CLNT_SLA.CREATED_BY_ID IS 'CREATED_BY_ID:';

LABEL ON COLUMN CAL_CLNT_SLA.LAST_UPDT_TMSTP IS 'LAST_UPDT_TMSTP:';

LABEL ON COLUMN CAL_CLNT_SLA.LAST_UPDT_BY_ID IS 'LAST_UPDT_BY_ID:';

CREATE UNIQUE INDEX CALCS ON CAL_CLNT_SLA
(
 CLNT_ID              ASC, 
 CLNT_PROCESSNG_TYPE_CD ASC
);

ALTER TABLE CAL_CLNT_SLA
 ADD CONSTRAINT CLNT_SLA  PRIMARY KEY (CLNT_ID,
 CLNT_PROCESSNG_TYPE_CD);

CREATE TABLE CAL_DT_TEST1
(
 CLNT_ID              CHARACTER( 3 ) NOT NULL,
 PROCESSING_DATE      DATE NOT NULL WITH DEFAULT '0001-01-01'
);

LABEL ON COLUMN CAL_DT_TEST1.CLNT_ID IS 'CLNT_ID:';

LABEL ON COLUMN CAL_DT_TEST1.PROCESSING_DATE IS 'PROCESSING_DATE:';

CREATE TABLE CAL_INV_TRN_SUM_HIS1
(
 CLNT_ID              CHARACTER( 3 ) NOT NULL,
 ACCT_ID              CHARACTER( 19 ) NOT NULL,
 SECURITY_ID          CHARACTER( 12 ) NOT NULL,
 TRN_SUMHIS_CREATED_ON_TMSTP TIMESTAMP( 6 ) WITHOUT TIME ZONE NOT NULL
 WITH DEFAULT,
 STLMNT_ID            CHARACTER( 8 ) NOT NULL,
 OPNG_SECURITY_QTY    INTEGER NOT NULL,
 OPNG_SECURITY_QTY_DT DATE NOT NULL,
 CLSNG_SECURITY_QTY   INTEGER NOT NULL,
 CLSNG_SECURITY_QTY_DT DATE NOT NULL,
