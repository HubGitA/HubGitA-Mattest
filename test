CREATE TABLE CAL_TST
(
 A                    CHARACTER( 10 ) NOT NULL,
 B                    INTEGER NOT NULL,
 C                    DATE NOT NULL,
 D                    CHARACTER( 3 ) NOT NULL
);

LABEL ON COLUMN CAL_TST.A IS 'A:';

LABEL ON COLUMN CAL_TST.B IS 'B:';

LABEL ON COLUMN CAL_TST.C IS 'C:';

LABEL ON COLUMN CAL_TST.D IS 'D:';

CREATE UNIQUE INDEX calts1 ON CAL_TST
(
 A                    ASC
);

ALTER TABLE CAL_TST
 ADD CONSTRAINT A  PRIMARY KEY (A);

